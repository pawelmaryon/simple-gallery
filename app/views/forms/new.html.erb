<%= simple_form_for (@form) do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :"description" %>
  <%= f.text_field :"description" %>
  <%= f.label :mobile %>
  <%= f.text_field :mobile %>
  <%= f.submit %>
<% end %>
